## LLM: set LLM_PROVIDER to "openai" or "vertex", then set the matching variables below ##
LLM_PROVIDER=openai

## OpenAI-Compatible Server (when LLM_PROVIDER=openai) ##
OPENAI_API_KEY=sk-dummy
OPENAI_MODEL_NAME=llama3
OPENAI_BASE_URL=http://localhost:11434/v1

## Vertex AI / Claude on Vertex (when LLM_PROVIDER=vertex) ##
# VERTEX_PROJECT_ID=
# VERTEX_REGION=us-central1
# VERTEX_MODEL_NAME=claude-3-5-sonnet@20241022

## Authentication ##
# Generate using `chainlit create-secret`
CHAINLIT_AUTH_SECRET=

## MLFlow ##
MLFLOW_TRACKING_URI=
MLFLOW_EXPERIMENT_NAME="Agent Demo"
MLFLOW_SYSTEM_PROMPT_URI=
MLFLOW_GENAI_EVAL_MAX_WORKERS=2
MLFLOW_GENAI_EVAL_MAX_SCORER_WORKERS=4

